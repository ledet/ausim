<?xml version="1.0" encoding="UTF-8"?>
<project name="XSLT_Single" default="xslt">
	<property name="rootModelName" value="RoboSoccer" />
	<property name="modelName" value="RoboSoccer" />
	<property name="modelPath" value="../Models/IntermediateModels/" />
	<property name="transPath" value="Simulink2SysML" />

	<target name="xslt">
		<xslt style="${transPath}/XSLT/SysMLFinalizeDI.xsl"
			in="${modelPath}/${rootModelName}/${modelName}.notation"
			out="${modelPath}/${rootModelName}/${modelName}.di"
			force="true">
			<param name="ModelName" expression="${modelName}"/>
		</xslt>

	</target>

</project>